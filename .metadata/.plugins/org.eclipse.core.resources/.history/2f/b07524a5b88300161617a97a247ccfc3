
public class Test {
	public static void main(String args[]) {
		System.out.println(compute("sac", "abcde"));
	}

	private static String compute(String string1, String string2) {

		if (string1.length() == 1)
			return string1;

		if (string2.length() == 1)
			return string2;

		for (int i = string1.length(); i > 0; i--) {
			for (int j = string2.length(); j > 0; j--) {
				if (string1.charAt(i - 1) == string2.charAt(j - 1)) {
					System.out.println("String 1: " + string1.substring(0, string1.length() - 1));
					System.out.println("String 2: " + string2.substring(0, j - 1));

					return compute(string1.substring(0, string1.length() - 1), string2.substring(0, j - 1))
							+ string1.charAt(i - 1);
				}
			}
		}
		return "";
	}
}
