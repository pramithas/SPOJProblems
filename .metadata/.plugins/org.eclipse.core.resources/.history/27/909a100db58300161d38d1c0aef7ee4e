
public class Test {
	public static void main(String args[]) {
		System.out.println(compute("ace", "abcde"));
	}

	private static String compute(String string1, String string2) {

		for (int i = string1.length(); i > 0; i--) {
			for (int j = string2.length(); j > 0; j--) {
				if (string1.charAt(i - 1) == string2.charAt(j - 1)) {
					System.out.println("Strign 1" + string1.substring(0, string1.length() - i));
					System.out.println("String 2" + string2.substring(0, j));
					return string1.charAt(i - 1)
							+ compute(string1.substring(0, string1.length() - i), string2.substring(0, j));
				}
			}
			return compute(string1, string2.substring(0, string2.length()));
		}
		return "";
	}
}
